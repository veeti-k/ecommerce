{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "clean": {
      "cache": false
    },
    "start": {
      "dependsOn": ["^start"]
    },
    "db:pushall":{
      "dependsOn": ["^db:pushall"],
      "cache": false
    },
    "dev": {
      "dependsOn": ["^dev"]
    },
    "test":{
      "dependsOn": ["^test"],
      "cache": false
    },
    "seed": {
      "dependsOn": ["^seed"],
      "cache": false
    } 
  }
}