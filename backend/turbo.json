{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "clean": {
      "cache": false
    },
    "start": {
      "dependsOn": ["^start"]
    },
    "test":{
      "dependsOn": ["^test"]
    },
    "db:pushall":{
      "dependsOn": ["^db:pushall"]
    },
    "dev": {
      "dependsOn": ["^dev"]
    }
  }
}