  },
  "dependencies": {
    "@types/node": "^17.0.25",
    "@types/express": "^4.17.13",
    "@prisma/client": "^3.12.0",
    "axios": "^0.26.1",
    "express": "^4.17.3",
    "typescript": "^4.6.3",
    "config": "*",
    "shared": "*",
    "tsconfig": "*"
  },
  "devDependencies": {
    "prisma": "^3.12.0"
  }
}
