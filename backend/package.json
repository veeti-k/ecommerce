{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && ./node_modules/.bin/tsc -p ./tsconfig.json",
    "dev": "yarn build && yarn prisma db push && yarn prisma generate && node ./dist/index.js",
    "devinit": "yarn && yarn build && yarn prisma db init && yarn prisma generate && node ./dist/index.js",
    "start": "yarn build && node ./dist/index.js",
    "clean": "rm -rf node_modules && rm -rf .turbo && rm -rf dist",
    "test": "jest"
  },
  "prisma": {
    "seed": "yarn ts-node ./prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "ts-jest": "^27.1.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.25",
    "jest": "^27.5.1",
    "prisma": "^3.12.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
