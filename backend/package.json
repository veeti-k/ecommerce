{
  "name": "backend",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && ./node_modules/.bin/tsc -p ./tsconfig.json",
    "dev": "yarn build && node --experimental-specifier-resolution=node ./dist/index.js",
    "start": "yarn build && node --experimental-specifier-resolution=node ./dist/index.js",
    "clean": "rm -rf node_modules && rm -rf .turbo && rm -rf dist",
    "test": "jest"
  },
  "prisma": {
    "seed": "node ./prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.25",
    "babel-jest": "^27.5.1",
    "jest": "^27.5.1",
    "prisma": "^3.12.0",
    "typescript": "^4.6.3"
  }
}
