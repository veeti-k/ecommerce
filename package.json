{
  "name": "ecommerce",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:front": "yarn turbo run dev:front",
    "build:front": "yarn turbo run build:front",
    "start:front": "yarn turbo run start:front",
    "lint:front": "yarn turbo run lint:front",
    "test:backend": "docker compose -f docker-compose.test.yml up --abort-on-container-exit && docker-compose -f docker-compose.test.yml rm -fsv",
    "clean": "turbo run clean && rm -rf node_modules",
    "build": "yarn && turbo run build"
  },
  "devDependencies": {
    "turbo": "^1.2.5"
  },
  "engines": {
    "npm": ">=8.7.0",
    "node": ">=17.8.0"
  },
  "packageManager": "yarn@1.22.18"
}
