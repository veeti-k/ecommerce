{
  "name": "ecommerce",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "yarn && turbo run build",
    "clean": "turbo run clean && rm -rf node_modules",
    "test:backend": "docker compose -f docker-compose.test.yml up --abort-on-container-exit && docker-compose -f docker-compose.test.yml rm -fsv",
    "start": "turbo run start",
    "buildprod": "yarn turbo run buildprod"
  },
  "devDependencies": {
    "turbo": "^1.2.5"
  },
  "engines": {
    "npm": ">=8.7.0",
    "node": ">=17.8.0"
  },
  "packageManager": "yarn@1.22.18"
}
